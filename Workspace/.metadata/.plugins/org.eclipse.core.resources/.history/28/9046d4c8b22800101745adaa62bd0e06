#include "../inc/reg.h"
/*
* (1U << 10)  // set bit 10 to 1
* &= ~(1U << 11) // set bit 11 to 0
*/


int main(void)
{
    /* 1. Enable clock access to GPIOA
    *  2. Set PA5 as output pin 
    *  3. Set PA5 high
    */
   RCC_AHB1EN_R |= GPIOAEN;
   GPIOA_MODE_R |= (1U << 10);
   GPIOA_MODE_R &= ~(1U << 11);



    while (1)
    {
        //GPIOA_OD_R |= LED_PIN;
        GPIOA_OD_R ^= LED_PIN;
        for (long long i = 0; i < 100000; i++)
        ;
        
    }
    
}
